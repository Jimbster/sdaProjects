<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.rent.myapplication">
<uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE"/>
    <uses-permission android:name="android.permission.INTERNET"/>

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".drawing.DrawingActivity"></activity>
        <activity android:name=".gallery.GalleryActivity"></activity>
        <activity android:name=".todolist.TodoListActivity"
            android:label="todolist!"></activity>
        <activity android:name=".quiz.QuizActivity" />
        <activity android:name=".BookActivity"/>
    </application>

</manifest>